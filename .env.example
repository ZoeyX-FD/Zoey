# coingecko API Keys
COINGECKO_API_KEY=    #u must add this if u use crypto-agents

#providers api keys
DEEPSEEK_API_KEY=
GEMINI_API_KEY=
MISTRAL_API_KEY=
OPENAI_API_KEY=
OPENROUTER_API_KEY=


# Provider Selection for crypto-agents
TECHNICAL_PROVIDER=openrouter    # Options: openai, gemini, mistral, cohere, deepseek , openrouter
FUNDAMENTAL_PROVIDER=openrouter
SENTIMENT_PROVIDER=openrouter
SYNOPSIS_PROVIDER=openrouter
EXTRACTOR_PROVIDER=mistral

# Model Selection for crypto-agents (optional - will use defaults if not specified)
TECHNICAL_MODEL=deepseek/deepseek-r1:free
FUNDAMENTAL_MODEL=google/gemini-2.0-flash-001
SENTIMENT_MODEL=deepseek/deepseek-r1:free	
SYNOPSIS_MODEL=google/gemini-2.0-flash-001
EXTRACTOR_MODEL=mistral-large-latest

# System Settings
MINUTES_BETWEEN_ROUNDS=30
MAX_HISTORY_ROUNDS=50

# Memory Settings
AGENT_MEMORY_DIR=data/agent_memory

# Social Media Agent
TWITTER_USERNAME=
TWITTER_PASSWORD=
TWITTER_EMAIL=
TWITTER_COOKIE_STRING=


# Sentiment Analysis Settings
SENTIMENT_DEFAULT_TARGETS=BTC,ETH,SOL
SENTIMENT_ADDITIONAL_TARGETS=SOL,SUI
SENTIMENT_INCLUDE_TRENDING=true
SENTIMENT_MAX_POSTS=100
SENTIMENT_MIN_ENGAGEMENT=10
SENTIMENT_LANGUAGES=en
SENTIMENT_INCLUDE_RETWEETS=false

# Topic Analysis Settings
TOPIC_PROVIDER=openrouter
TOPIC_MODEL=openai/gpt-4o-mini
TOPIC_UPDATE_INTERVAL=60  # minutes
TOPIC_MIN_CONFIDENCE=0.7
TOPIC_ANALYSIS_TARGETS="Solana,Ethereum,Bitcoin,DeFi"

AUTO_FOLLOW=true

EXA_API_KEY=your_exa_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Clients to run
CLIENTS = "twitter" #for run twitter bot

TWITTER_CONFIG_PATH=/root/(nameyourfolder)/config/twitter_config.json


